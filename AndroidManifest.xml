<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drorg8.calculatoractivity"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />-->
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:name="caltulatorApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.drorg8.calculatoractivity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.drorg8.calculatoractivity.PrefListActivity"
            android:label="@string/title_activity_pref_list" >
        </activity>
        <activity
            android:name="com.drorg8.calculatoractivity.HistoryListActivity"
            android:label="@string/title_activity_history_list" >
        </activity>
    </application>

</manifest>